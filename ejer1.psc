Algoritmo examen_2
	definir x Como Entero;
	Escribir "ingrese el valor de x ";
	leer x;
	Escribir "ingrese el valor de a ";
	leer  a;
	Escribir "valor original ";
	f=a*(x)^3+3;
	Escribir "Y = ",f;
	Escribir "opciones";
	b=a*x*x*x+3;
	c=a*x*x*(x+3);
	d=(a*x)*x*(x+3);
	e=a*(x*x*x)+3;
	
	Escribir "a.Y = ",b;
	Escribir "b.Y = ",c;
	Escribir "c.Y = ",d;
	Escribir "d.Y = ",e;
	
	Escribir "respuesta a y d";
FinAlgoritmo
